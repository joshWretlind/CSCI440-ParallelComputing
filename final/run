#!/bin/bash

mpiexec -machinefile machines -np 1 ./main_exe 256 "The quick brown fox jumps over the lazy dog"

echo " "

echo "Let's hash Paradise Lost, by John Milton"
echo "using 256 bit outputs: "
for i in `seq 1 2`;
do
    foobar=$(cat paradiseLost.txt | tr "\"" "\\"\" | tr "'" "\'")
    string+=$foobar
done    
mpiexec -machinefile machines -np 1 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 2 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 3 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 4 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 5 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 6 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 7 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 8 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 9 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 10 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 11 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 12 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 13 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 14 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 15 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 16 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 17 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 18 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 19 ./main_exe 256 \"$string\"
echo " "
mpiexec -machinefile machines -np 20 ./main_exe 256 \"$string\"
echo " "

echo "using 512 bit outputs"
mpiexec -machinefile machines -np 1 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 2 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 3 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 4 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 5 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 6 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 7 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 8 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 9 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 10 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 11 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 12 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 13 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 14 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 15 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 16 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 17 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 18 ./main_exe 512 \"$string\"
echo " "
mpiexec -machinefile machines -np 19 ./main_exe 512 \"$string\"
